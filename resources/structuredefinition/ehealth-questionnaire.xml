<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-questionnaire" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-questionnaire" />
  <name value="ehealth-questionnaire" />
  <title value="eHealth Questionnaire" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Questionnaire" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Questionnaire" />
  <derivation value="constraint" />
  <differential>
    <element id="Questionnaire">
      <path value="Questionnaire" />
      <short value="A structured set of questions intended to guide the collection of answers from end-users." />
    </element>
    <element id="Questionnaire.extension">
      <path value="Questionnaire.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Questionnaire.extension:recommendation">
      <path value="Questionnaire.extension" />
      <sliceName value="recommendation" />
      <short value="Degree to which this questionnaire is recommended for use." />
      <definition value="Degree to which this questionnaire is recommended for use." />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-questionnaire-recommendation" />
      </type>
    </element>
    <element id="Questionnaire.extension:intendedOrganization">
      <path value="Questionnaire.extension" />
      <sliceName value="intendedOrganization" />
      <short value="The organization intended to use this questionnaire." />
      <definition value="The organization intended to use this questionnaire." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-questionnaire-intendedOrganization" />
      </type>
    </element>
    <element id="Questionnaire.extension:revision">
      <path value="Questionnaire.extension" />
      <sliceName value="revision" />
      <short value="Business revision of the questionnaire." />
      <definition value="Business revision of the questionnaire." />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-revision" />
      </type>
    </element>
    <element id="Questionnaire.extension:modifierRole">
      <path value="Questionnaire.extension" />
      <sliceName value="modifierRole" />
      <min value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-modifier-role" />
      </type>
    </element>
    <element id="Questionnaire.extension:type">
      <path value="Questionnaire.extension" />
      <sliceName value="type" />
      <short value="Concept that represents the overall questionnaire" />
      <definition value="An identifier for this question or group of questions in a particular terminology such as LOINC." />
      <comment value="Codes may be defined very casually in enumerations or code lists, up to very formal definitions such as SNOMED CT - see the HL7 v3 Core Principles for more information." />
      <requirements value="Allows linking of the complete Questionnaire resources to formal terminologies.  It's common for &quot;panels&quot; of questions to be identified by a code." />
      <min value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-questionnaire-type" />
      </type>
    </element>
    <element id="Questionnaire.version">
      <path value="Questionnaire.version" />
      <min value="1" />
    </element>
    <element id="Questionnaire.jurisdiction">
      <path value="Questionnaire.jurisdiction" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="Jurisdiction" />
        </extension>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-isCommonBinding">
          <valueBoolean value="true" />
        </extension>
        <strength value="extensible" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/jurisdiction" />
        </valueSetReference>
      </binding>
    </element>
    <element id="Questionnaire.item.extension">
      <path value="Questionnaire.item.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Questionnaire.item.extension:minOccurs">
      <path value="Questionnaire.item.extension" />
      <sliceName value="minOccurs" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org/fhir/StructureDefinition/questionnaire-minOccurs" />
      </type>
    </element>
    <element id="Questionnaire.item.extension:minOccurs.valueInteger:valueInteger">
      <path value="Questionnaire.item.extension.valueInteger" />
      <sliceName value="valueInteger" />
      <min value="1" />
    </element>
    <element id="Questionnaire.item.extension:feedback">
      <path value="Questionnaire.item.extension" />
      <sliceName value="feedback" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-questionnaire-feedback" />
      </type>
    </element>
    <element id="Questionnaire.item.extension:maxOccurs">
      <path value="Questionnaire.item.extension" />
      <sliceName value="maxOccurs" />
