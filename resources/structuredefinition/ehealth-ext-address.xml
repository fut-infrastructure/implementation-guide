<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-address" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-address" />
  <name value="DanishAddress" />
  <title value="eHealth Address" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <description value="Danish address including municipality code and regional code" />
  <fhirVersion value="3.0.1" />
  <kind value="complex-type" />
  <abstract value="false" />
  <type value="Address" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Address" />
  <derivation value="constraint" />
  <differential>
    <element id="Address.extension">
      <path value="Address.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Address.extension:municipalityCode">
      <path value="Address.extension" />
      <sliceName value="municipalityCode" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-municipality-code" />
      </type>
    </element>
    <element id="Address.extension:regionalSubdivisionCode">
      <path value="Address.extension" />
      <sliceName value="regionalSubdivisionCode" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-regional-subdivision-code" />
      </type>
    </element>
    <element id="Address.use">
      <path value="Address.use" />
      <min value="1" />
    </element>
    <element id="Address.district">
      <path value="Address.district" />
      <max value="0" />
    </element>
    <element id="Address.state">
      <path value="Address.state" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>