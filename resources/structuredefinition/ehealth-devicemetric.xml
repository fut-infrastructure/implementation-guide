<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-devicemetric" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric" />
  <name value="ehealth-devicemetric" />
  <title value="eHealth DeviceMetric" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="DeviceMetric" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/DeviceMetric" />
  <derivation value="constraint" />
  <differential>
    <element id="DeviceMetric">
      <path value="DeviceMetric" />
      <short value="Describes a measurement, calculation or setting capability of a medical device." />
    </element>
    <element id="DeviceMetric.extension">
      <path value="DeviceMetric.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="DeviceMetric.extension:properties">
      <path value="DeviceMetric.extension" />
      <sliceName value="properties" />
      <short value="Properties of the Device" />
      <definition value="Properties are static characteristics of the Device and do not change over time or across calibration cycles, such as color, weight, ease-of-use, language, CE-labelling, etc." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric-properties" />
      </type>
    </element>
    <element id="DeviceMetric.extension:quality">
      <path value="DeviceMetric.extension" />
      <sliceName value="quality" />
      <short value="Qualities of the Device" />
      <definition value="Qualities are characteristics of the Device which may change over time or across calibration cycles, such as battery state, etc." />
      <min value="1" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric-qualities" />
      </type>
    </element>
    <element id="DeviceMetric.extension:qualitiesHistory">
      <path value="DeviceMetric.extension" />
      <sliceName value="qualitiesHistory" />
      <short value="History of device quality" />
      <definition value="History of device quality. This is maintained by the system." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric-qualityHistory" />
      </type>
    </element>
    <element id="DeviceMetric.extension:templateQualities">
      <path value="DeviceMetric.extension" />
      <sliceName value="templateQualities" />
      <min value="3" />
      <max value="3" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric-templateQualities" />
      </type>
    </element>
    <element id="DeviceMetric.type">
      <path value="DeviceMetric.type" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="MetricType" />
        </extension>
        <strength value="preferred" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/device-measurement-unit" />
        </valueSetReference>
      </binding>
    </element>
    <element id="DeviceMetric.unit">
      <path value="DeviceMetric.unit" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="MetricUnit" />
        </extension>
        <strength value="preferred" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/device-measurement-unit" />
        </valueSetReference>
      </binding>
    </element>
    <element id="DeviceMetric.source">
      <path value="DeviceMetric.source" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-device" />
        <aggregation value="referenced" />
      </type>
    </element>
    <element id="DeviceMetric.parent">
      <path value="DeviceMetric.parent" />
      <max value="0" />
    </element>
    <element id="DeviceMetric.calibration">
      <path value="DeviceMetric.calibration" />
      <short value="Describes the calibrations that have been performed or that are required to be performed." />
    </element>
    <element id="DeviceMetric.calibration.extension">
      <path value="DeviceMetric.calibration.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="DeviceMetric.calibration.extension:calibrationExpires">
      <path value="DeviceMetric.calibration.extension" />
      <sliceName value="calibrationExpires" />
      <short value="Date when the current calibration expires." />
      <definition value="Date when the current calibration expires and recalibration is required." />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-devicemetric-calibrationExpires" />
      </type>
    </element>
  </differential>
</StructureDefinition>