    <element id="Patient.address">
      <path value="Patient.address" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="use" />
        </discriminator>
        <rules value="open" />
      </slicing>
      <min value="1" />
    </element>
    <element id="Patient.address:officialHomeAddress">
      <path value="Patient.address" />
      <sliceName value="officialHomeAddress" />
      <min value="1" />
      <max value="1" />
      <type>
