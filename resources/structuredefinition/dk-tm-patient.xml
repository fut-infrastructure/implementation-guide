      <fixedCode value="other" />
    </element>
    <element id="Patient.telecom:eBoks.value">
      <path value="Patient.telecom.value" />
      <min value="1" />
      <fixedString value="eBoks" />
    </element>
    <element id="Patient.telecom:eBoks.use">
      <path value="Patient.telecom.use" />
      <max value="0" />
    </element>
    <element id="Patient.telecom:eBoks.rank">
      <path value="Patient.telecom.rank" />
      <max value="0" />
    </element>
    <element id="Patient.gender">
      <path value="Patient.gender" />
      <min value="1" />
