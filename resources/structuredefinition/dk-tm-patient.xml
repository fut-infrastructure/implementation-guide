    <element id="Patient.telecom:NemSMS.rank">
      <path value="Patient.telecom.rank" />
      <max value="0" />
    </element>
    <element id="Patient.telecom:eBoks">
      <path value="Patient.telecom" />
      <sliceName value="eBoks" />
    </element>
    <element id="Patient.telecom:eBoks.system">
      <path value="Patient.telecom.system" />
