<?xml version="1.0" encoding="UTF-8"?>
<OperationDefinition xmlns="http://hl7.org/fhir" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <id value="ehealth-appointment-toical-operationdefinition"/>
    <name value="Export Appointment to iCal"/>
    <status value="active"/>
    <kind value="operation"/>
    <date value="2019-02-02"/>
    <description value="This operation exports the Appointment to an iCal [RFC-5545](https://tools.ietf.org/html/rfc5545) format"/>
    <code value="exportAsiCal"/>
    <comment value="The iCal file is wrapped inside a Binary resource."/>
    <resource value="Appointment"/>
    <system value="false"/>
    <type value="false"/>
    <instance value="true"/>
    <parameter>
        <name value="return"/>
        <use value="out"/>
        <min value="1"/>
        <max value="1"/>
        <documentation value="Bundle including a Binary resource that encapsulates the iCal file in a Binary resource"/>
        <type value="Bundle"/>
    </parameter>
</OperationDefinition>